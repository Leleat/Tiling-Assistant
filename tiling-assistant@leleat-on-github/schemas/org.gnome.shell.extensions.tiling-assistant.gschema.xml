<?xml version="1.0" encoding="UTF-8"?>
<schemalist>
	<schema id="org.gnome.shell.extensions.tiling-assistant" path="/org/gnome/shell/extensions/tiling-assistant/">

		<!-- General Page -->

		<key name="enable-tiling-popup" type="b">
			<default>true</default>
		</key>
		<key name="tiling-popup-all-workspace" type="b">
			<default>false</default>
		</key>
		<key name="enable-raise-tile-group" type="b">
			<default>true</default>
		</key>
		<key name="tilegroups-in-app-switcher" type="b">
			<default>false</default>
		</key>
		<key name="dynamic-keybinding-behavior" type="i">
			<!-- 0: Disabled, 1: Focus, 2: Tiling State, 3: Tiling State (Windows), 4: Favorite Layout -->
			<default>0</default>
		</key>
		<key name="focus-hint" type="i">
			<default>0</default>
		</key>
		<key name="focus-hint-color" type="s">
			<default>''</default>
		</key>
		<key name="focus-hint-outline-border-radius" type="i">
			<default>8</default>
		</key>
		<key name="focus-hint-outline-size" type="i">
			<default>8</default>
		</key>
		<key name="window-gap" type="i">
			<default>0</default>
		</key>
		<key name="single-screen-gap" type="i">
			<default>0</default>
		</key>
		<key name="screen-top-gap" type="i">
			<default>0</default>
		</key>
		<key name="screen-left-gap" type="i">
			<default>0</default>
		</key>
		<key name="screen-right-gap" type="i">
			<default>0</default>
		</key>
		<key name="screen-bottom-gap" type="i">
			<default>0</default>
		</key>
		<key name="maximize-with-gap" type="b">
			<default>false</default>
		</key>
		<key name="monitor-switch-grace-period" type="b">
			<default>true</default>
		</key>

		<!-- Keybindings Page -->

		<key name="toggle-tiling-popup" type="as">
			<default>[]</default>
		</key>
		<key name="tile-edit-mode" type="as">
			<default>[]</default>
		</key>
		<key name="auto-tile" type="as">
			<default>[]</default>
		</key>
		<key name="toggle-always-on-top" type="as">
			<default>[]</default>
		</key>
		<key name="tile-maximize" type="as">
			<default><![CDATA[['<Super>Up','<Super>KP_5']]]></default>
		</key>
		<key name="tile-maximize-vertically" type="as">
			<default>[]</default>
		</key>
		<key name="tile-maximize-horizontally" type="as">
			<default>[]</default>
		</key>
		<key name="restore-window" type="as">
			<default><![CDATA[['<Super>Down']]]></default>
		</key>
		<key name="center-window" type="as">
			<default>[]</default>
		</key>
		<key name="tile-top-half" type="as">
			<default><![CDATA[['<Super>KP_8']]]></default>
		</key>
		<key name="tile-bottom-half" type="as">
			<default><![CDATA[['<Super>KP_2']]]></default>
		</key>
		<key name="tile-left-half" type="as">
			<default><![CDATA[['<Super>Left','<Super>KP_4']]]></default>
		</key>
		<key name="tile-right-half" type="as">
			<default><![CDATA[['<Super>Right','<Super>KP_6']]]></default>
		</key>
		<key name="tile-topleft-quarter" type="as">
			<default><![CDATA[['<Super>KP_7']]]></default>
		</key>
		<key name="tile-topright-quarter" type="as">
			<default><![CDATA[['<Super>KP_9']]]></default>
		</key>
		<key name="tile-bottomleft-quarter" type="as">
			<default><![CDATA[['<Super>KP_1']]]></default>
		</key>
		<key name="tile-bottomright-quarter" type="as">
			<default><![CDATA[['<Super>KP_3']]]></default>
		</key>
		<key name="tile-top-half-ignore-ta" type="as">
			<default>[]</default>
		</key>
		<key name="tile-bottom-half-ignore-ta" type="as">
			<default>[]</default>
		</key>
		<key name="tile-left-half-ignore-ta" type="as">
			<default>[]</default>
		</key>
		<key name="tile-right-half-ignore-ta" type="as">
			<default>[]</default>
		</key>
		<key name="tile-topleft-quarter-ignore-ta" type="as">
			<default>[]</default>
		</key>
		<key name="tile-topright-quarter-ignore-ta" type="as">
			<default>[]</default>
		</key>
		<key name="tile-bottomleft-quarter-ignore-ta" type="as">
			<default>[]</default>
		</key>
		<key name="tile-bottomright-quarter-ignore-ta" type="as">
			<default>[]</default>
		</key>
		<!-- Layouts Page -->
		<!-- Hidden by default unless the advanced / experimental settings are enabled.
		The number of layouts / shortcuts are hardcoded / limited to 20 -->

		<key name="import-layout-examples" type="b">
			<default>true</default>
		</key>
		<key name="show-layout-panel-indicator" type="b">
			<default>false</default>
		</key>
		<key name="search-popup-layout" type="as">
			<default>[]</default>
		</key>
		<key name="favorite-layout" type="i">
			<default>-1</default>
		</key>
		<key name="favorite-layouts" type="as">
			<default>[]</default>
		</key>
		<key name="activate-layout0" type="as">
			<default>[]</default>
		</key>
		<key name="activate-layout1" type="as">
			<default>[]</default>
		</key>
		<key name="activate-layout2" type="as">
			<default>[]</default>
		</key>
		<key name="activate-layout3" type="as">
			<default>[]</default>
		</key>
		<key name="activate-layout4" type="as">
			<default>[]</default>
		</key>
		<key name="activate-layout5" type="as">
			<default>[]</default>
		</key>
		<key name="activate-layout6" type="as">
			<default>[]</default>
		</key>
		<key name="activate-layout7" type="as">
			<default>[]</default>
		</key>
		<key name="activate-layout8" type="as">
			<default>[]</default>
		</key>
		<key name="activate-layout9" type="as">
			<default>[]</default>
		</key>
		<key name="activate-layout10" type="as">
			<default>[]</default>
		</key>
		<key name="activate-layout11" type="as">
			<default>[]</default>
		</key>
		<key name="activate-layout12" type="as">
			<default>[]</default>
		</key>
		<key name="activate-layout13" type="as">
			<default>[]</default>
		</key>
		<key name="activate-layout14" type="as">
			<default>[]</default>
		</key>
		<key name="activate-layout15" type="as">
			<default>[]</default>
		</key>
		<key name="activate-layout16" type="as">
			<default>[]</default>
		</key>
		<key name="activate-layout17" type="as">
			<default>[]</default>
		</key>
		<key name="activate-layout18" type="as">
			<default>[]</default>
		</key>
		<key name="activate-layout19" type="as">
			<default>[]</default>
		</key>

		<!-- Hidden Settings Page -->

		<key name="enable-advanced-experimental-features" type="b">
			<default>false</default>
		</key>
		<key name="enable-tile-animations" type="b">
			<default>true</default>
		</key>
		<key name="enable-untile-animations" type="b">
			<default>true</default>
		</key>
		<key name="disable-tile-groups" type="b">
			<default>false</default>
		</key>
		<key name="default-move-mode" type="i">
			<!-- 0: Edge Tiling, 1: Adaptive Tiling, 2: Favorite Layout, 3: Ignore TA -->
			<default>0</default>
		</key>
		<key name="low-performance-move-mode" type="b">
			<default>false</default>
		</key>
		<key name="adapt-edge-tiling-to-favorite-layout" type="b">
			<default>false</default>
		</key>
		<key name="move-adaptive-tiling-mod" type="i">
			<!-- 0: Disabled, 1: Ctrl, 2: Alt, 3: RMB, 4: Meta -->
			<default>1</default>
		</key>
		<key name="move-favorite-layout-mod" type="i">
			<!-- 0: Disabled, 1: Ctrl, 2: Alt, 3: RMB, 4: Meta -->
			<default>2</default>
		</key>
		<key name="ignore-ta-mod" type="i">
			<!-- 0: Disabled, 1: Ctrl, 2: Alt, 3: RMB, 4: Meta -->
			<default>0</default>
		</key>
		<key name="vertical-preview-area" type="i">
			<default>15</default>
		</key>
		<key name="horizontal-preview-area" type="i">
			<default>15</default>
		</key>
		<key name="toggle-maximize-tophalf-timer" type="i">
			<default>600</default>
		</key>
		<key name="enable-hold-maximize-inverse-landscape" type="b">
			<default>false</default>
		</key>
		<key name="enable-hold-maximize-inverse-portrait" type="b">
			<default>false</default>
		</key>
		<key name="debugging-show-tiled-rects" type="as">
			<default>[]</default>
		</key>
		<key name="debugging-free-rects" type="as">
			<default>[]</default>
		</key>
		<key name="last-version-installed" type="i">
			<default>-1</default>
		</key>
		<key name="overridden-settings" type="a{sv}">
			<!-- Private key for remembering the overridden values -->
			<default>[]</default>
		</key>
	</schema>
</schemalist>
